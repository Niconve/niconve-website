# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# JWT Secret (for token signing - minimum 32 characters)
JWT_SECRET=your_jwt_secret_here_minimum_32_characters_long

# Midtrans Payment Gateway
# Get these from: https://dashboard.midtrans.com/settings/config_info
# Use sandbox keys for testing, production keys for live
MIDTRANS_SERVER_KEY=your_midtrans_server_key_here
MIDTRANS_CLIENT_KEY=your_midtrans_client_key_here

# Base URL (for payment callbacks)
# Development: http://localhost:3000
# Production: https://your-domain.vercel.app
BASE_URL=http://localhost:3000

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=900000

# Admin Password
ADMIN_PASSWORD=t1d4kt4u

# Node Environment (development | production)
NODE_ENV=development
